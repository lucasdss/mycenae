{
  "services": [
    {
      "ID": "mycenae",
      "Name": "mycenae",
      "Tags": [
        "http"
      ],
      "Port": 8080,
      "Token": "your-token-here",
      "Check": {
        "ID": "mycenae-probe",
        "Name": "mycenae-probe",
